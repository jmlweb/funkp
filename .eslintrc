{
    "env": {
      "es6": true,
      "jasmine": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "extends": ["airbnb-base"],
    "rules": {
      "arrow-parens": 0,
      "function-paren-newline": 0,
      "object-curly-newline": 0,
      "no-mixed-operators": 0
    }
  }