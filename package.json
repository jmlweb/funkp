{
  "name": "pfunk",
  "version": "0.0.1",
  "description": "P-Funk - Utils for JavaScript FP coding style",
  "keywords": [
    "fp",
    "javascript",
    "functional programming",
    "pfunk",
    "utility belt"
  ],
  "homepage": "https://jmlweb.github.io/pfunk/",
  "main": "cjs/index.js",
  "umd:main": "cjs/index.umd.js",
  "module": "src/index.js",
  "browser": "cjs/index.js",
  "repository": "git@github.com:jmlweb/pfunk.git",
  "author": "JoseÃÅ Manuel Lucas <josemanuel@jmlweb.es>",
  "license": "MIT",
  "engines": {
    "node": ">=6",
    "npm": ">= 3.10.10"
  },
  "scripts": {
    "test": "nyc --check-coverage --lines 95 --functions 95 --branches 95 --reporter=text --reporter=html mocha --require @babel/register --timeout=3000 src/**/*.spec.js",
    "test:dev": "mocha --watch --require @babel/register src/**/*.spec.js",
    "clean": "rimraf ./cjs",
    "prebuild": "npm run clean",
    "build": "microbundle",
    "build:dev": "microbundle -w",
    "doc": "jsdoc --configure .jsdoc.json --verbose",
    "predoc:serve": "npm run doc",
    "doc:serve": "serve -n -o docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run doc && git add . --all"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/register": "^7.0.0-beta.47",
    "assert": "^1.4.1",
    "babel-eslint": "^8.2.3",
    "cross-env": "^5.1.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.12.0",
    "husky": "^1.0.0-rc.6",
    "jsdoc": "^3.5.5",
    "microbundle": "^0.7.0",
    "mocha": "^5.2.0",
    "nyc": "^11.8.0",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "serve": "^6.5.7"
  },
  "dependencies": {}
}
